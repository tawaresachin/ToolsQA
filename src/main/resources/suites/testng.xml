<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="DemoQATest" parallel="tests" thread-count="10" group-by-instances="true" preserve-order="true">
    <listeners>
        <listener class-name="com.cybage.assignment.objects.AnnotationTransformer"/>
    </listeners>

    <test name="toolsqaHomeTest" >
        <classes>
           <class name="com.cybage.assignment.test.toolsqaHomePageTest"/>
        </classes>
    </test>
    <test name="toolsqaElementTest">
        <classes>
            <class name="com.cybage.assignment.test.toolsqaElementPageTest"/>
        </classes>
    </test>
    <test name="toolsqaFormTest">
        <classes>
            <class name="com.cybage.assignment.test.toolsqaFormPageTest"/>
        </classes>
    </test>
    <test name="toolsqaWindowTest" >
        <classes>
            <class name="com.cybage.assignment.test.alertFrameWindowPageTest"/>
        </classes>
    </test>
    <test name="toolsqaWidgetTest">
        <classes>
            <class name="com.cybage.assignment.test.widgetsPageTest"/>
        </classes>
    </test>
</suite>
